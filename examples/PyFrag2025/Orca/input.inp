JOBSUB

#!/bin/bash
#SBATCH -J orca_pyfrag
#SBATCH -t 5:00
#SBATCH -N 1
#SBATCH --tasks-per-node=16
#SBATCH --partition=tc
#SBATCH --output=orca.out
#SBATCH --error=orca.err

module load shared
module swap 2025 2022
module load orca/6.0.0

export NSCM=16

export WORK_TMPDIR=$TMPDIR
cd $WORK_TMPDIR

JOBSUB End


INPUT_SPECS
type = IRC
output file = irc.amv
fa1_name = pd_cc_ts
frag1 = pd
1.H
2.H
end frag1
frag2 = cc
3.H
4.H
end frag2

print bond 1 3 1.00
print strain frag1  100
print strain frag2  200

END INPUT_SPECS

! SP B3LYP 6-31G(d)

* xyz 0 1
END INPUT
