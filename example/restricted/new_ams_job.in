##### Uses the  >AMS2019 inputfile format (indicated by the AMS and AMS END keys)
##### You must load the ams (>2019) module

JOBSUB

#!/bin/bash
#SBATCH -N 1
#SBATCH -t 00:10:00
#SBATCH --ntasks-per-node=16
#SBATCH --partition=genoa
#SBATCH --output=new_ams_job.out
#SBATCH --error=new_ams_job.err

# TC
# module load ams/2023.101

# Snellius
# module load 2022
# module load AMS/2023.101-intelmpi
# module list


JOBSUB END


PyFrag
name new_ams_job
ircpath molecule.xyz
fragment  2
fragment  1 3 4 5 6
strain    0
strain   -554.09
bondlength 1 6  1.09

PyFrag END

AMS

Task SinglePoint

Engine adf
  basis
    core Small
    type TZ2P
  End

  eprint
    sfo EIG OVL
  End

  numericalquality VeryGood

  xc
    gga OPBE
  End
EndEngine

AMS END